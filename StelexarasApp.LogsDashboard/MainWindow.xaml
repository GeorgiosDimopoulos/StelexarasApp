<Window x:Class="StelexarasApp.LogsDashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Main Dashboard" Height="450" Width="800">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="*" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<Button Content="View Error Logs" Click="ViewErrorLogs_Click" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
		<Button Content="View CRUD Logs" Click="ViewCrudLogs_Click" Grid.Row="1" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
	</Grid>
</Window>