<Window x:Class="StelexarasApp.LogsDashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StelexarasApp.LogsDashboard"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<DataGrid x:Name="LogsDataGrid" AutoGenerateColumns="True" HeadersVisibility="Column" ItemsSource="{Binding LogEntries}" LoadingRow="LogsDataGrid_LoadingRow">
		<DataGridTextColumn Header="Type" Binding="{Binding Type}"/>
		<DataGridTextColumn Header="Message" Binding="{Binding MethodName}"/>
		<DataGridTextColumn Header="Message" Binding="{Binding Message}"/>
		<DataGridTextColumn Header="Timestamp" Binding="{ Binding Timestamp}"/>
	</DataGrid>
</Window>
